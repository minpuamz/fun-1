class Solution {
    public void reverseString(char[] s) {
        if (s == null || s.length == 0) return;
        int len = s.length;
        for(int i = 0; i < len/2; i++) {
            char tmp = s[i];
            s[i] = s[len - i - 1];
            s[len - i - 1] = tmp;
        }
        return;
    }
}
