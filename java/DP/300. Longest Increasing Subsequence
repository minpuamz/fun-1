// dp[i] the LIS ends in i;
// dp[j]= Max(i, dp[i] + 1 for all i if nums[i] < nums[j] && i < j])
//Time O(n*n)
//Spac O(n)
class Solution {
    public int lengthOfLIS(int[] nums) {
        if (nums == null) return 0;
        int len = nums.length;
        int res = 0;
    
        int[] dp = new int[len];
        for (int i = 0; i < len; i++) {
            dp[i] = 1;
            for (int j = 0; j < i; j++) {
                if (nums[j] < nums[i]) {
                     dp[i] = Math.max(dp[i], dp[j]+1);
                }
            }
            res = Math.max(res, dp[i]);
        }
        return res;
    }
}
